<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content">
    <View
        android:background="@color/gray_f2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingLeft="11dp"
        android:paddingRight="11dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="33dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:text="张三"
                android:textSize="12sp"
                android:textColor="@color/view_bg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_centerInParent="true"
                android:layout_marginLeft="5dp"
                android:text="所属部门：市场部"
                android:textSize="12sp"
                android:textColor="@color/gray_69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:text="区域：A2-12"
                android:textSize="12sp"
                android:textColor="@color/gray_69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:background="@color/gray_f2"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="33dp">
            <ImageView
                android:id="@+id/iv"
                android:layout_centerVertical="true"
                android:src="@mipmap/dingdanshouyi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_toRightOf="@+id/iv"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:text="订单收益"
                android:textSize="12sp"
                android:textColor="@color/gray_69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:text="0.00"
                android:textSize="12sp"
                android:textColor="@color/view_bg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:background="@color/gray_f2"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="33dp">
            <ImageView
                android:id="@+id/iv1"
                android:layout_centerVertical="true"
                android:src="@mipmap/tuikuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_toRightOf="@+id/iv1"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:text="售后退款"
                android:textSize="12sp"
                android:textColor="@color/gray_69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:text="0.00"
                android:textSize="12sp"
                android:textColor="@color/view_bg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:background="@color/gray_f2"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="33dp">
            <ImageView
                android:id="@+id/iv2"
                android:layout_centerVertical="true"
                android:src="@mipmap/baifang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_toRightOf="@+id/iv2"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:text="拜访次数"
                android:textSize="12sp"
                android:textColor="@color/gray_69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:text="0.00"
                android:textSize="12sp"
                android:textColor="@color/view_bg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:background="@color/gray_f2"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="33dp">
            <ImageView
                android:id="@+id/iv3"
                android:layout_centerVertical="true"
                android:src="@mipmap/zhuce"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_toRightOf="@+id/iv3"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:text="注册"
                android:textSize="12sp"
                android:textColor="@color/gray_69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:text="21家"
                android:textSize="12sp"
                android:textColor="@color/view_bg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:background="@color/gray_f2"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="33dp">
            <ImageView
                android:id="@+id/iv4"
                android:layout_centerVertical="true"
                android:src="@mipmap/fuhuo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_toRightOf="@+id/iv4"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:text="复活"
                android:textSize="12sp"
                android:textColor="@color/gray_69"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:text="21家"
                android:textSize="12sp"
                android:textColor="@color/view_bg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:background="@color/gray_f2"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="42dp">

            <ImageView
                android:id="@+id/tv_call"
                android:src="@mipmap/call"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_phone"
                android:layout_toRightOf="@+id/tv_call"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/gray_66"
                android:layout_marginLeft="11dp"
                android:text="18616833332"
                android:layout_height="wrap_content" />
            <Button
                android:id="@+id/tv_status"
                android:layout_width="35dp"
                android:gravity="center"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textSize="10sp"
                android:textColor="@color/white"
                android:background="@mipmap/zaizhi"
                android:layout_marginLeft="16dp"
                android:text="在职"
                android:layout_height="17dp" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>